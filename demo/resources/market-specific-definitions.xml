         <!--field name="QuoteCondition"        tag="marketspecific.fields.5" type="QuoteConditionEnum"/-->
      </fields>
      <types>
         <enums>
           <enum name="HedgeSpecEnum"  type="char" tag="marketspecific.types.1">
              <value name="Hedger"                 value="0" label="Hedger" />
              <value name="Speculator"             value="1" label="Speculator" />
           </enum>
           <!--enum name="QuoteConditionEnum"  type="char" tag="marketspecific.types.5">
              <value name="Regular"         value=" " label="Regular Quote" />
              <value name="Rotation"        value="R" label="Rotation Quote" />
              <value name="TradingHalt"     value="T" label="Trading Halt" />
           </enum-->
           <enum name="CpfFlagEnum"  type="char" tag="marketspecific.types.6">
              <value name="CPF"                 value="x" label="CPF trade" />
              <value name="FirstTranche"        value="s" label="1st tranche" />
              <value name="SecondTranche"       value="2" label="2nd tranche" />
              <value name="SRS"                 value="e" label="SRS trade" />
           </enum>
         </enums>
      </types>
   </namespace>
</service-definitions>
